<odoo>
    <record id="act_report_xml_view_inherit_agent" model="ir.ui.view">
        <field name="name">ir.actions.report.form.inherit.agent</field>
        <field name="model">ir.actions.report</field>
        <field name="inherit_id" ref="base.act_report_xml_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="agent_printer_name" placeholder="Ex: EPSON"/>
            </xpath>
        </field>
    </record>
</odoo>